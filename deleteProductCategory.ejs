<% include layout %>

<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title"> <%=title %>  </h3>
    </div>

    <div class="panel-body">

        <div class="container"
             data-ng-app="CategoryModule"
             data-ng-controller="deleteCategoryCtrl">

            <div class="alert alert-info">
                    Are you sure you want to delete this product Category?
                    <button class="btn btn-danger"  data-ng-click="deleteCategory()">Yes</button>
                    <button class="btn btn-primary" data-ng-click="">Cancel</button>                   
            </div>       

        </div>
    </div>
  </div>

    <script src="../bower_components/angular/angular.min.js"></script>
    <script src="../../javascripts/app/Category/CategoryModule.js"></script>
    <script src="../../javascripts/app/Category/CategoryService.js"></script>
    <script src="../../javascripts/app/Category/deleteCategoryCtrl.js"></script>















